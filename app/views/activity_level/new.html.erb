<h1>New Activity Level</h1>

<%= form_with model: @activity_level, url: activity_level_index_path do |f| %>
  <div class="field">
    <%= f.label :vitamin_c %><br />
    <%= f.number_field :vitamin_c, in: 1..3, step: 1 %>
  </div>

  <div class="field">
    <%= f.label :vitamin_d3 %><br />
    <%= f.number_field :vitamin_d3, in: 1..3, step: 1 %>
  </div>

  <div class="field">
    <%= f.label :iron %><br />
    <%= f.number_field :iron, in: 1..3, step: 1 %>
  </div>

  <br />

  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>

<%= link_to "Back", :back %>
